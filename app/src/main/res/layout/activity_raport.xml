<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".client.Raport">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/main"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:titleTextColor="@color/main"
        tools:ignore="MissingConstraints,NewApi"
        tools:targetApi="jelly_bean">

        <ImageView
            android:id="@+id/picture"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:background="@color/main"
            android:foregroundGravity="left"
            android:padding="0dp"
            android:src="@drawable/fundal"
            tools:ignore="ContentDescription" />
    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contact_toolbar"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:columnCount="3"
                android:rowCount="5">

                <Button
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="20dp"
                    android:layout_row="0"
                    android:layout_column="0"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/button_pdf"
                    app:backgroundTint="@null"/>
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:layout_marginLeft="18dp"
                    android:text="RAPORT PERMISIE"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="11dp"/>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="20dp"
                    android:layout_row="0"
                    android:layout_column="1"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/button_pdf"
                    app:backgroundTint="@null"/>
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:layout_marginLeft="18dp"
                    android:text="RAPORT PĂRĂSIRE ȚARĂ"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="11dp"/>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="15dp"
                    android:layout_row="0"
                    android:layout_column="2"
                    android:background="@drawable/button_pdf"
                    app:backgroundTint="@null"/>
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:layout_row="1"
                    android:layout_column="2"
                    android:layout_marginLeft="18dp"
                    android:text="RAPORT DESCAZARMARE"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="11dp"/>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="20dp"
                    android:layout_row="2"
                    android:layout_column="0"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/button_pdf"
                    app:backgroundTint="@null"/>
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:layout_row="3"
                    android:layout_column="0"
                    android:layout_marginLeft="18dp"
                    android:text="RAPORT DECONTARE MEDICAMENTE"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="11dp"/>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="20dp"
                    android:layout_row="2"
                    android:layout_column="1"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/button_pdf"
                    app:backgroundTint="@null"/>
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:layout_row="3"
                    android:layout_column="1"
                    android:layout_marginLeft="18dp"
                    android:text="RAPORT PRIMIRE LAPTOP"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="11dp"/>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="15dp"
                    android:layout_row="2"
                    android:layout_column="2"
                    android:background="@drawable/button_pdf"
                    app:backgroundTint="@null"/>
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:layout_row="3"
                    android:layout_column="2"
                    android:layout_marginLeft="18dp"
                    android:text="RAPORT TRANSPORT LAPTOP"
                    android:textColor="@color/black"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="11dp"/>
            </GridLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="134dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/illustration" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>





</androidx.constraintlayout.widget.ConstraintLayout>